<h1>All Users</h1>
<header>
  <nav>
    <%= link_to 'Create New User', new_user_path %> | 
    <%= link_to 'Sign In', new_session_path %> |
    <%= link_to 'Sign Out', session_path('active'), method: :delete %>
  </nav>
</header>

<% @users.each do |user| %>
  <ul>
    <li>
      <%= link_to user.name, user_path(user) %>
    </li>
  </ul>
<% end %>